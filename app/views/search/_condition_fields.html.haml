= content_tag :fieldset, {  class: 'fields', 'data-object-name' => f.object_name   } do
  %legend
    = link_to_remove_fields t('remove'),f
  = f.attribute_fields do |a|
    = render "attribute_fields",f:a
  .large-4.small-4.columns
    = f.select :p, {'等于' => :eq, '小于等于' => :lteq,'小于'=> :lt, '大于等于'=>:gteq,'大于'=>:gt,'包含'=>:cont,'开头为'=>:start, '是否存在'=>:present}
    -#= f.predicate_select
  = f.value_fields do |v|
    = content_tag(:span, v.text_field(:value),  { class: 'fields value large-4 small-4 columns', 'data-object-name' => v.object_name }, false)